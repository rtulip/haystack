
fn sys_write(u64 ptr u64) -> [u64] {
    1 syscall(3)
}

fn write(Str) {
    split 1 sys_write drop
}

struct Words {
    Str: hello
    Str: world
}

fn main() {
    "Hello\n" "World!\n" cast(Words)
    var [words]

    words::hello write
    words::world write
    words::hello::size print
    words::world::size print
    

}