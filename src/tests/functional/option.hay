fn main() {

    "Hello World" Option.Some as [someStr]

    &someStr Option.is_some println
    &someStr Option.is_none println
    someStr Option.unwrap println 

    Option.None::<Str> as [noneStr] 
    &noneStr Option.is_some println
    &noneStr Option.is_some println

    noneStr Option.unwrap println

}