include "std.hay"
include "opt.hay"

fn do_work(Opt<u64>: maybe) -> [Opt<bool>] {

    &maybe Opt.is_none if {
        Opt.None::<bool> return
    }

    while true {
        maybe Opt.unwrap 10 < Opt.Some return 
    }
    
    Opt.None::<bool>
}

fn main() {

    9 Opt.Some do_work Opt.unwrap  println
    12 Opt.Some do_work Opt.unwrap println
    Opt.None::<u64> do_work as [opt] 
    &opt Opt.is_none println

}