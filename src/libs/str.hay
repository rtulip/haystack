
include "write.hay"
include "print.hay"

impl Write<u8> {
    fn write(u8: n u64: fd) {
        n cast(u64) fd write
    }
}

impl Write<bool> {
    fn write(bool: b u64: fd) {
        b if { "true" } else { "false" }
        fd write
    }
}

impl Write<char> {
    fn write(char: c u64: fd) {
        1 &c cast(Str) fd write
    }
}


