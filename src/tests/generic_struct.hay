include "std.hay"

struct Foo {
    u64: bar
}

struct Pair<T> {
    T: first 
    T: second
}

struct Quad<A B> {
    Pair<A>: a
    Pair<B>: b
}

fn into_quad<A B>(Pair<A> Pair<B>) -> [Quad<A B>] {
    cast(Quad)
}

fn main() {

    // stack: [u64 u64] => Pair<u64>
    1 2 cast(Pair)

    "World\n" "Hello\n" cast(Pair)

    into_quad
    split 
    split write write 
    split print print

}