fn do_work(Option<u64>: maybe) -> [Option<bool>] {

    &maybe Option.is_none if {
        Option.None::<bool> return
    }

    while true do {
        maybe Option.unwrap 10 < Option.Some return 
    }
    
    Option.None::<bool>
}

fn main() {

    9 Option.Some do_work Option.unwrap  println
    12 Option.Some do_work Option.unwrap println
    Option.None::<u64> do_work as [opt] 
    &opt Option.is_none println

}