include "opt.hay"
include "std.hay"
fn main() {

    "Hello World" Opt.Some as [someStr]

    &someStr Opt.is_some println
    &someStr Opt.is_none println
    someStr Opt.unwrap println 

    Opt.None::<Str> as [noneStr] 
    &noneStr Opt.is_some println
    &noneStr Opt.is_some println

    noneStr Opt.unwrap println

}